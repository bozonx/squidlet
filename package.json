{
  "name": "squidlet",
  "version": "5.14.0",
  "description": "Easy make IoT devices and local net of devices which can be managed by master",
  "author": "Ivan Kozyrin <ipkozyrin@gmail.com>",
  "repository": "git@github.com:bozonx/squidlet.git",
  "engines": {
    "node": ">=10.9.0"
  },
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./control/index.ts",
    "buildPigpio": "cd platforms/nodejs/pigpioImage && docker build -t 'bozonx/pigpio' .",
    "buildProdImage-x86": "updater/buildProdImage.sh x86",
    "buildProdImage-rpi": "updater/buildProdImage.sh rpi",
    "buildProdImage-arm": "updater/buildProdImage.sh arm",
    "buildUpdater-x86": "./squidletLight/squidletLight.sh --output-dir=./build/updater-x86 --machine=x86 --platform=nodejs $@ ./updater/updaterHostConfig.yaml",
    "buildUpdater-rpi": "./squidletLight/squidletLight.sh --output-dir=./build/updater-rpi --machine=rpi --platform=nodejs $@ ./updater/updaterHostConfig.yaml",
    "buildUpdater-arm": "./squidletLight/squidletLight.sh --output-dir=./build/updater-arm --machine=arm --platform=nodejs $@ ./updater/updaterHostConfig.yaml",
    "test": "mocha",
    "lint": "tslint -p tsconfig.json",
    "postinstall": "cd node_modules && ln -s ../system ./"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "immutable": "^4.0.0-rc.12",
    "js-yaml": "^3.14.0",
    "jsmodbus": "^4.0.4",
    "lodash": "^4.17.21",
    "mqtt": "^4.2.0",
    "mqtt-packet": "^6.3.2",
    "pigpio-client": "^1.5.1",
    "serialport": "^9.0.1",
    "shelljs": "^0.8.4",
    "ws": "^7.3.1",
    "yaml": "^1.10.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-json-strings": "^7.10.4",
