#!/usr/bin/env bash

DIRNAME=$(dirname $0)
export NODE_PATH=${DIRNAME}/node_modules:${NODE_PATH}

export NVM_DIR="$HOME/.nvm" && [ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"

/usr/bin/env node ${DIRNAME}/squidletctl.js ${@}

